{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Projects"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "projectTitle": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo-component.seo"
    },
    "thumbnail": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "author": {
      "type": "string"
    },
    "timestamp": {
      "type": "date"
    },
    "authorIcon": {
      "type": "string"
    },
    "project_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project-category.project-category",
      "inversedBy": "projects"
    }
  }
}
